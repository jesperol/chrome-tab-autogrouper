{
  "name": "Tab Grouper",
  "description": "Automatically places new tabs in a group so that the active tab gets easily idenifiable, even with dark theme.", 
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "tabGroups", 
    "storage"
  ], 
  "background": {
    "service_worker": "service_worker.js"
  },
  "icons": {
    "48": "icons/group_dark-48.png"
  },
  "action": {
    "browser_style": true,
    "default_icon": {
      "48": "icons/group_dark-48.png"
    }
  }
}
